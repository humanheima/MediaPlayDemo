<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/activity_main"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_start_video_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="启动VideoActivity"/>

    <Button
        android:id="@+id/btn_start_live_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_start_video_activity"
        android:text="启动直播界面"/>

    <Button
        android:id="@+id/btn_start_music_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_start_live_activity"
        android:text="启动播放音乐界面"/>
</RelativeLayout>
